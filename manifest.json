{

  "manifest_version": 2,
  "name": "AutoTimeframeColors",
  "version": "1.5",
  "author": "Tiqur",

  "description": "Changes line color according to current timeframe",

  "permissions": ["storage", "clipboardWrite"],

  "content_scripts": [
    {
      "matches": ["*://*.tradingview.com/*"],
      "js": ["main.js", "background.js"],
      "css": ["styles.css"]
    }
  ]
}
